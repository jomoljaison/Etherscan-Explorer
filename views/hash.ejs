    }
    .box{
        display: inline-block;
        padding: 0%;
        width: 300px;
        background-color: rgba(243, 239, 239, 0.938);

    }



    input {
    padding: 4px 10px;
    border: 0;
    font-size: 19px;
    width: 55%;
    
  }
